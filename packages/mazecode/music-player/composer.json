{
    "name": "mazecode/music-player",
    "description": "Music player package",
    "keywords": [
        "music",
        "player",
        "song",
        "album"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "mazecode",
            "email": "diego.feliud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Mazecode\\MusicPlayer\\": "src/"
        },
        "classmap": [
            "src/database/seeds",
            "src/database/factories"
        ]
    },
    "autoload-dev": {

        "psr-4": {
            "Mazecode\\MusicPlayer\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mazecode\\MusicPlayer\\MusicPlayerServiceProvider"
            ],
            "aliases": {
                "MusicPlayer": "Mazecode\\MusicPlayer\\Facades\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
